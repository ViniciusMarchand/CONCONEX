{
  "expo": {
    "scheme": "conconex",
    "name": "conconex",
    "slug": "conconex",
    "version": "1.0.0",
    "main": "./App/App.tsx",
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "conconex",
            "host": "oauthredirect"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ],
    "web": {
      "favicon": "./Assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
     "plugins": [
      "expo-font", 
      "@react-native-google-signin/google-signin", 
      [ 
        "expo-build-properties", 
        { 
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-secure-store" 
    ],

    "orientation": "portrait",
    "icon": "./Assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./Assets/logos/icone_dark_mode.png",
      "resizeMode": "contain",
      "backgroundColor": "#111c28"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.conconex"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./Assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "usesCleartextTraffic": true,
      "package": "com.conconex"
    },
    "extra": {
      "eas": {
        "projectId": "5bec6ab9-00ad-40b4-811b-b2b42b8bb400"
      }
    },
    "owner": "vinimarchand"
  }
}

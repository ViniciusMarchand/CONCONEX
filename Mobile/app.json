{
  "expo": {
    "scheme": "conconex",
    "name": "conconex",
    "slug": "conconex",
    "version": "1.0.0",
    "main": "./App/App.tsx",
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "conconex",
            "host": "oauthredirect"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ],
    "web": {
      "favicon": "./Assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-font",
        "@react-native-google-signin/google-signin"
      ],
      "expo-secure-store"
    ],
    "orientation": "portrait",
    "icon": "./Assets/logos/icone_dark_mode.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./Assets/logos/icone_dark_mode.png",
      "resizeMode": "contain",
      "backgroundColor": "#111c28"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.conconex"
    },
    "extra": {
      "eas": {
        "projectId": "5bec6ab9-00ad-40b4-811b-b2b42b8bb400"
      }
    },
    "owner": "vinimarchand"
  }
}